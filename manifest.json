{
	"name": "Canaille",
	"id": "canaille",
	"packaging_format": 1,
	"description": {
		"en": "OpenID anthentificator against LDAP directoru",
		"fr": "Service d'authentification OpenID basé sur un annuaire LDAP" 
	},
	"version": "0.0.6~ynh1",
	"url": "https://canaille.readthedocs.io/",
	"upstream": {
        "license": "AGPL-3.0",
        "website": "https://canaille.readthedocs.io/",
        "admindoc": "https://canaille.readthedocs.io/",
        "userdoc": "https://canaille.readthedocs.io/",
        "code": "https://gitlab.com/yaal/canaille"
    },
	"license": "AGPL-3.0",
	"maintainer": {
		"name": "YunoHost Contributors",
		"email": "apps@yunohost.org"
	},
	"requirements": {
		"yunohost": ">= 4.2.0"
	},
	"multi_instance": false,
	"services": [
		"nginx"
	],
	"arguments": {
		"install" : [
			{
				"name": "domain",
				"type": "domain"
			},
			{
				"name": "path",
				"type": "path",
				"example": "/auth",
				"default": "/"
			},
			{
				"name": "is_public",
				"type": "boolean",
				"help": {
					"en": "Any YunoHost user and anonymous people from the web will be able to access the application",
					"fr": "Tout utilisateur YunoHost et les personnes anonymes pourront accéder à l'application"
				},
				"default": true
			}
		]
	}
}
